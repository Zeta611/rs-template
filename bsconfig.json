{
  "name": "rs-template",
  "version": "0.1.0",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "package-specs": {
    "module": "es6-global",
    "in-source": true
  },
  "suffix": ".js",
  "jsx": { "version": 4, "mode": "automatic" },
  "bs-dependencies": [
    "@rescript/react"
  ],
  "warnings": {
    "error" : "+101"
  },
  "bsc-flags": ["-open Belt"]
}
